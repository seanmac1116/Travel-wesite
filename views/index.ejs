<body>
    
<!-- header section starts  -->

<header class="header">

    <a href="#" class="logo"> Texas </a>

    <form action="" class="search-form">
        <input type="search" name="" placeholder="search here..." id="searchBox">
        <label for="searchBox" class="fas fa-search"></label>
    </form>

    <div class="icons">
        <div class="fas fa-search" id="search-btn"></div>
        <div class="fas fa-moon" id="theme-btn"></div>
        <div class="fas fa-user" id="login-btn"></div>
        <div class="fas fa-bars" id="menu-btn"></div>
    </div>

    <nav class="navbar">
        <a href="#home">home</a>
        <a href="#packages">packages</a>
        <a href="#services">services</a>
        <a href="#review">review</a>
        <a href="#contact">contact</a>
        <a href="#blogs">blogs</a>
    </nav>

    <form action="" class="login-form">

        <div class="inputBox">
            <span>username</span>
            <input type="text" placeholder="enter your name">
        </div>

        <div class="inputBox">
            <span>password</span>
            <input type="password" placeholder="enter your password">
        </div>

        <div class="remember">
            <input type="checkbox" name="" id="login-remember">
            <label for="login-remember">remember me</label>
        </div>

        <input type="submit" class="btn" value="login">

    </form>

</header>

<!-- header section ends -->

<!-- home section starts  -->

<section class="home" id="home">

    <div class="image" data-aos="fade-down">
        <img src="/images/ccimage-shutterstock_1022694613.jpg" alt="">
    </div>

    <div class="content" data-aos="fade-up">
        <h3>Deep in the heart of Texas</h3>
        <p>If you're looking to discover a world unlike your own, Come visit Texas. Texas has some of the best food, landmark, and attractions</p>
        <a href="#" class="btn">explore now</a>
    </div>

</section>

<!-- home section ends -->

<!-- filter form section starts  -->

<section class="form-container" data-aos="zoom-in">

    <form action="">

        <div class="inputBox">
            <span>where to</span>
            <input type="text" placeholder="search places">
        </div>

        <div class="inputBox">
            <span>arrivals</span>
            <input type="date">
        </div>

        <div class="inputBox">
            <span>leaving</span>
            <input type="date">
        </div>

        <input type="submit" value="book now" class="btn">

    </form>

</section>

<!-- filter form section ends -->

<!-- packages section starts  -->

<section class="packages" id="packages">

    <h1 class="heading"> our <span>packages</span> </h1>

    <div class="box-container">

        <div class="box" data-aos="fade-up">
            <div class="image">
                <img src="/images/San Antonio.jpg" alt="">
                <h3> <i class="fas fa-map-marker-alt"></i> san antonio </h3>
            </div>
            <div class="content">
                <div class="price"> $200 </div>
                <p>You can beat this price to visit the Alamo City. Military town USA.</p>
                <a href="#" class="btn"> book now</a>
            </div>
        </div>

        <div class="box" data-aos="fade-up">
            <div class="image">
                <img src="/images/Dallas.jpg" alt="">
                <h3> <i class="fas fa-map-marker-alt"></i> dallas </h3>
            </div>
            <div class="content">
                <div class="price"> $200 </div>
                <p>The home of the Ewings and the Dallas Cowboys.</p>
                <a href="#" class="btn"> book now</a>
            </div>
        </div>

        <div class="box" data-aos="fade-up">
            <div class="image">
                <img src="/images/Houston.jpg" alt="">
                <h3> <i class="fas fa-map-marker-alt"></i> houston </h3>
            </div>
            <div class="content">
                <div class="price"> $200 </div>
                <p>Houston the home of big oil and the Texans.</p>
                <a href="#" class="btn"> book now</a>
            </div>
        </div>

        <div class="box" data-aos="fade-up">
            <div class="image">
                <img src="/images/El Paso.jpeg" alt="">
                <h3> <i class="fas fa-map-marker-alt"></i> el paso </h3>
            </div>
            <div class="content">
                <div class="price"> $200 </div>
                <p>Great border town with great people.</p>
                <a href="#" class="btn"> book now</a>
            </div>
        </div>

        

    </div>

</section>

<!-- packages section ends -->

<!-- services section starts  -->

<section class="services" id="services">

    <h1 class="heading"> our <span>services</span> </h1>

    <div class="box-container">

        <div class="box" data-aos="zoom-in">
            <i class="fas fa-hotel"></i>
            <h3>hotels</h3>
            <p>Best hotels, and the lowest prices!!!.</p>
        </div>

        <div class="box" data-aos="zoom-in">
            <i class="fas fa-plane"></i>
            <h3>travel</h3>
            <p>The quickest way to get you too Texas, for the lowest price.</p>
        </div>

        <div class="box" data-aos="zoom-in">
            <i class="fas fa-utensils"></i>
            <h3>meals</h3>
            <p>Food and drink are included in the price.</p>
        </div>

        <div class="box" data-aos="zoom-in">
            <i class="fas fa-globe"></i>
            <h3>all over the lone star state</h3>
            <p>We want you to experience everuthing that Texas has to offer.</p>
        </div>

    

    </div>

</section>

<!-- services section ends -->

<!-- pricing section starts  -->



